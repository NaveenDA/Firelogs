{
    "name": "firelogs",
    "version": "1.0.1",
    "description": "",
    "private": true,
    "main": "index.js",
    "scripts": {
        "prestart": "node scripts/prestart.js",
        "start": "webpack --watch",
        "serve": "webpack serve",
        "build": "cross-env NODE_ENV=production webpack build && node scripts/build.js",
        "format": "prettier  --write \"./src/**/*.{js,hbs,scss}\""
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "jquery": "^3.6.0",
        "jqueryui": "^1.11.1",
        "localforage": "^1.9.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "css-loader": "^5.2.6",
        "file-loader": "^6.2.0",
        "fs-extra": "^10.0.0",
        "handlebars": "^4.7.7",
        "handlebars-loader": "^1.7.1",
        "husky": "^6.0.0",
        "import-sort-cli": "^6.0.0",
        "import-sort-style-renke": "^6.0.0",
        "lint-staged": "^11.0.0",
        "prettier": "^2.3.1",
        "pretty-quick": "^3.1.0",
        "sass": "^1.34.1",
        "sass-loader": "^12.0.0",
        "style-loader": "^2.0.0",
