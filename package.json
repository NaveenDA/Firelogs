        "webpack": "^5.31.1",
        "webpack-cli": "^4.7.2",
        "webpack-dev-server": "^3.11.2"
    },
    "prettier": {
        "trailingComma": "none"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "importSort": {
        ".js, .jsx, .ts, .tsx": {
            "style": "renke"
